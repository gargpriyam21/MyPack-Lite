<h1>Waitlists</h1>

<table>
  <thead>
  <tr>
    <th> Student ID</th>
    <th> Course Code</th>
    <th colspan="1"></th>
  </tr>
  </thead>
  <tbody>
  <% @waitlists.each do |waitlist| %>
    <tr>
      <td><%= waitlist.student_code %></td>
      <td><%= waitlist.course_code %></td>
      <td><%= link_to "Show this waitlist", waitlist %></td>
  <% end %>
  </tr>
  </tbody>
</table>

<br>
<%= link_to "New Enrollment", new_enrollment_path %>
